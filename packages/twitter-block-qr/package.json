{
  "name": "twitter-block-qr",
  "version": "1.0.0",
  "scripts": {
    "build": "esno build.ts",
    "dev": "esno build.ts -w",
    "deploy": "liuli-cli deploy"
  },
  "deploy": {
    "type": "gh-pages",
    "dist": "dist",
    "dest": "/twitter-block-qr"
  },
  "userjs": {
    "name": "twitter-block-qr",
    "version": "0.1.0",
    "description": "屏蔽 twitter 上垃圾二维码消息",
    "author": "rxliuli",
    "match": [
      "https://twitter.com/*"
    ],
    "grant": [
      "unsafeWindow"
    ],
    "license": "MIT"
  },
  "devDependencies": {
    "@liuli-util/async": "^3.2.0",
    "@liuli-util/cli": "^3.18.0",
    "@types/fs-extra": "^9.0.13",
    "@types/greasemonkey": "^3.0.0",
    "esbuild": "^0.14.22",
    "esno": "^0.14.1",
    "fs-extra": "^10.0.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "qr-scanner": "^1.4.1"
  }
}
