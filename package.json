{
  "name": "userjs",
  "version": "1.0.0",
  "description": "一些吾辈日常浏览网页需要用到便自行实现的 userjs 脚本",
  "repository": "git@github.com:rxliuli/userjs.git",
  "author": "rxliuli",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "yarn workspaces foreach -p exec rimraf dist release",
    "build": "yarn workspaces foreach -p --topological-dev run build",
    "deploy": "yarn workspaces foreach -p run deploy"
  },
  "workspaces": [
    "packages/*"
  ],
  "prettier": "@liuli-util/prettier-standard-config",
  "devDependencies": {
    "@liuli-util/prettier-standard-config": "^0.1.0",
    "@types/greasemonkey": "^3.0.0",
    "lerna": "^3.22.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rx-util": "^1.9.2",
    "ts-node": "^9.0.0",
    "type-fest": "^0.21.2",
    "typescript": "^4.4.2"
  },
  "packageManager": "yarn@3.0.0-rc.2"
}
